    <script>
    $('#enquiryForm').on('submit', async (e) => {
      e.preventDefault();

      // Fetching form values
      const fullName = $('#fullName').val().trim();
      const email = $('#email').val().trim();
      const contactNo = $('#contactNo').val().trim();
      const message = $('#message').val().trim();
      // const referenceId = $('#referenceId').val().trim();
      const referenceType = $('#referenceType')?.val()?.trim() || "ContactUs";
      // const referenceName = $('#referenceName').val().trim();
      // Validate required fields
      if (!fullName || !email || !contactNo || !message) {
        $('#enquiry_err').html('<div class="text-red-500 text-sm">Please fill all required fields.</div>');
        return;
      }

      // Validate Indian 10-digit mobile number (should start with 6, 7, 8, or 9)
      const contactRegex = /^[6-9]\d{9}$/;
      if (!contactRegex.test(contactNo)) {
        $('#enquiry_err').html('<div class="text-red-500 text-sm">Please enter a valid 10-digit Indian mobile number.</div>');
        return;
      }
      // Validate message length
      if (message.length > 400) {
        $('#enquiry_err').html('<div class="text-red-500 text-sm">Please minimize your message below 400 characters</div>');
        return;
      }
      // Update button state
      const submitButton = $('#submit_btn');
      submitButton.html(` <div
            class="inline-block h-4 w-4 animate-spin rounded-full border-2 border-solid border-current border-e-transparent align-[-0.125em] motion-reduce:animate-[spin_1.5s_linear_infinite]"
            role="status"></div>
          <span>Submitting...</span>`).prop('disabled', true).addClass('bg-green-800').removeClass('bg-theme2');

      try {
        const response = await fetch('/api/enquiry', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            fullName,
            email,
            contactNo,
            message,
            referenceType,
          })
        });
        const result = await response.json();

        if (response.ok) {
          $('#enquiry_err').html('<div class="text-green-500 text-sm">Enquiry submitted successfully.</div>');
          $('#enquiryForm').trigger('reset');
          setTimeout(() => {
            $('#enquiry_err').html('');
            // enquiryModal.hide();
            swal("Your Form Submitted Successfully!", "", "success");
          }, 100);
        } else {
          $('#enquiry_err').html(`<div class="text-red-500 text-sm">${result.message}</div>`);
        }
      } catch (error) {
        console.error('Error submitting enquiry:', error);
        $('#enquiry_err').html('<div class="text-red-500 text-sm">An error occurred. Please try again.</div>');
      } finally {
        // Reset button state
        submitButton.html('Submit').prop('disabled', false).addClass('bg-theme2').removeClass('bg-green-800');;
        setTimeout(() => {
          $('#enquiry_err').html('');
        }, 2000);
      }

    });
  </script>
  
